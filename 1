def nums_sum (*args):
    n_sum = 0
    p_sum = 0
    for num in args:
        if num > 0:
            p_sum += num
        elif num < 0:
            n_sum += num

    return n_sum, p_sum
nums= map(int, input().split())
neg_sum, pos_sum = nums_sum(*nums)

print(neg_sum)
print(pos_sum)

if abs(neg_sum) > abs(pos_sum):
    print(f"The negatives are stronger than the positives")
elif abs(pos_sum) > (neg_sum):
    print(f"The positives are stronger than the negatives")
